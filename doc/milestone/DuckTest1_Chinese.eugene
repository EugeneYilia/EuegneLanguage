函数 测试鸭子(){
    赵帅的鸭子的数量 = 3;
    赵衰的鸭子的数量 = 4 * 2;
    赵甩的鸭子的数量 = 0;
    打印(赵甩的鸭子的数量);

    帅衰两人的鸭子数量 = 赵帅的鸭子的数量 + 赵衰的鸭子的数量;

    赵帅有鸭子 = 1;
    赵衰有鸭子 = 1;
    赵甩有鸭子 = 0;
    如果(赵衰有鸭子) {
        打印(赵衰的鸭子的数量);
    } 否则 {
        打印(0);
    };

    如果(赵甩有鸭子) {
        打印(赵甩的鸭子的数量);
    } 否则 {
        打印(0);
    };
}

函数 入口() {
   打印(1010101010);
   测试鸭子();
   打印(1010101010);
}